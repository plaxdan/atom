{
  "initialZoom": 16,
  "initialCenter": [32.1613722, -110.8939361],
  "showScale": true,
  "classNames": [
    "icon-location-arrow": {
      "blah": "blah"
    }
    "work-orders_selected": {
      
    }
    "default-inspection-work-orders": {
      "marker": "circle",
      "stroke": true,
      "weight": 3,
      "color": "orange",
      "fillColor": "#ccc",
      "ds-rotate": "37.8deg"
    },
    "my-inspection-work-orders": {
      "marker": "circle",
      "stroke": true,
      "weight": 3,
      "color": "orange",
      "fillColor": "#ccc"
    },
    "other-inspection-work-orders": {
      "marker": "square",
      "stroke": true,
      "weight": 3,
      "color": "orange",
      "fillColor": "#ccc"
    },
    "primary-span_selected": {
      "stroke": true,
      "weight": 3,
      "color": "orange",
      "fillColor": "#ccc"
    }
  ],
  "locateControl": {
    "position": "topleft",
    "classSet": "icon-location-arrow",
    "follow": true,
    "stopFollowingOnDrag": true,
    "keepCurrentZoomLevel": true
  },
  "attributions": [
    "<a href=\"http://www.datasplice.com\">&copy; DataSplice, 2014</a>"
  ],
  "gpsSensor": {
    "endpoint": "http://localhost:8000/gps/location",
    "interval": 1000,
    "timeout": 30000
  },
  "legend": [
    "<div style='padding: 5px'><span style='border-bottom: 3px solid #e74c3c'>Assigned</span></div>",
    "<div style='background-color: #e08283; padding: 5px'>WAPPR</div>",
    "<div style='background-color: #27ae60; padding: 5px'>READY</div>",
    "<div style='background-color: #e67e22; padding: 5px'>INPRG</div>",
    "<div style='background-color: #2980b9; padding: 5px'>FIELDCOMP</div>",
    "<div style='background-color: #d35400; padding: 5px'>REWORK</div>",
    "<div style='background-color: #8e44ad; padding: 5px'>Open Labor</div>"
  ],
  "selection": {
    "dataMapping": "Latitude = ${lat} and Longitude = ${lng}"
  },
  "featureLayers": {
    "Major Streets": {
      "maxZoomVisible": 16,
      "classSet": "custom-major-street-style"
    },
    "Major Street Labels": {
      "minZoomVisible": 14,
      "maxZoomVisible": 15,
      "classSet": "class-name-from-above"
    },
    "User Streets": {
      "minZoomVisible": 14,
      "maxZoomVisible": 16,
      "classSet": "custom-user-street-style",
      "events": {
        "click": "Select Street"
      }
    },
    "Street Labels": {
      "minZoomVisible": 16,
    },
    "Primary Span": {
      "minZoomVisible": 16,
      "selection": {
        "groupBy": "CIRCUIT_NAME",
        "label": "Circuit ${CIRCUIT_NAME}",
        "classSet": "primary-span_selected",
        "relations": {
          "Some Layer": {
            "relatedWith": "SOME_SHARED_KEY"
            "classSet": "some-style-to-highlight-relations"
          },
          "Another Layer": {
            "relatedWith": {
              "PROP_OF_RELATION": "true",
              "PROP_OF_RELATION": "/regex/"
            }
            "classSet": {
              "style-to-highlight-related": true
              "other-style-to-highlight-related": "/regex/"
            }
          }
        }
      }
    },
    "Pole": {},
    "User Lot Line": {
      "minZoomVisible": 17
    },
    "User Right of Way": {
      "minZoomVisible": 16
    }
  },
  "dataLayers": {
    "Inspection Work Orders": {
      "query": "'Work Type' is 'Inspection'",
      "mapping": {
        "lat": "NAME_OF_FIELD_WITH_LAT",
        "lng": "NAME_OF_FIELD_WITH_LNG",
        "feature": {
          "layerName": "Premise",
          "properties": {
            "FEATURE_PROP_1": "DATA_PROP_1",
            "FEATURE_PROP_2": "DATA_PROP_2",
            "FEATURE_PROP_3": "DATA_PROP_3"
          }
        }
      },
      "classSet": {
        "default-inspection-work-orders": true,
        "my-inspection-work-orders": "${ASSIGNED_TO_ME}",
        "other-inspection-work-orders": "${ASSIGNED_TO_OTHERS}"
      },
      "selection": {
        "label": "${Description} (${Work Order})"
        "classSet": "work-orders_selected"
      }
    }
  }
}
